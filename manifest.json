{
    "manifest_version": 3,
    "name": "Event Generator",
    "version": "1.1",
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "options_page": "config.html"
  }
